---
{
   "apiVersion": "v1",
   "kind": "Namespace",
   "metadata": {
      "labels": {
         "app": "allennlp-demo",
         "contact": "allennlp-contact",
         "env": "staging"
      },
      "name": "allennlp-demo"
   }
}
---
{
   "apiVersion": "extensions/v1beta1",
   "kind": "Ingress",
   "metadata": {
      "annotations": {
         "certmanager.k8s.io/cluster-issuer": "letsencrypt-prod",
         "kubernetes.io/ingress.class": "nginx",
         "nginx.ingress.kubernetes.io/enable-cors": "false",
         "nginx.ingress.kubernetes.io/ssl-redirect": "true",
         "nginx.ingress.kubernetes.io/use-regex": "true"
      },
      "labels": {
         "app": "allennlp-demo",
         "contact": "allennlp-contact",
         "env": "staging"
      },
      "name": "allennlp-demo-staging-ingress",
      "namespace": "allennlp-demo"
   },
   "spec": {
      "rules": [
         {
            "host": "allennlp-demo.staging.apps.allenai.org",
            "http": {
               "paths": [
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-atis-parser",
                        "servicePort": 8000
                     },
                     "path": "/predict/atis-parser"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-constituency-parsing",
                        "servicePort": 8000
                     },
                     "path": "/predict/constituency-parsing"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-coreference-resolution",
                        "servicePort": 8000
                     },
                     "path": "/predict/coreference-resolution"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-dependency-parsing",
                        "servicePort": 8000
                     },
                     "path": "/predict/dependency-parsing"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-event2mind",
                        "servicePort": 8000
                     },
                     "path": "/predict/event2mind"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-fine-grained-named-entity-recognition",
                        "servicePort": 8000
                     },
                     "path": "/predict/fine-grained-named-entity-recognition"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-named-entity-recognition",
                        "servicePort": 8000
                     },
                     "path": "/predict/named-entity-recognition"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-naqanet-reading-comprehension",
                        "servicePort": 8000
                     },
                     "path": "/predict/naqanet-reading-comprehension"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-nlvr-parser",
                        "servicePort": 8000
                     },
                     "path": "/predict/nlvr-parser"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-open-information-extraction",
                        "servicePort": 8000
                     },
                     "path": "/predict/open-information-extraction"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-quarel-parser-zero",
                        "servicePort": 8000
                     },
                     "path": "/predict/quarel-parser-zero"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-reading-comprehension",
                        "servicePort": 8000
                     },
                     "path": "/predict/reading-comprehension"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-semantic-role-labeling",
                        "servicePort": 8000
                     },
                     "path": "/predict/semantic-role-labeling"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-textual-entailment",
                        "servicePort": 8000
                     },
                     "path": "/predict/textual-entailment"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-wikitables-parser",
                        "servicePort": 8000
                     },
                     "path": "/predict/wikitables-parser"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging",
                        "servicePort": 8000
                     },
                     "path": "/.*"
                  }
               ]
            }
         },
         {
            "host": null,
            "http": {
               "paths": [
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-atis-parser",
                        "servicePort": 8000
                     },
                     "path": "/predict/atis-parser"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-constituency-parsing",
                        "servicePort": 8000
                     },
                     "path": "/predict/constituency-parsing"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-coreference-resolution",
                        "servicePort": 8000
                     },
                     "path": "/predict/coreference-resolution"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-dependency-parsing",
                        "servicePort": 8000
                     },
                     "path": "/predict/dependency-parsing"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-event2mind",
                        "servicePort": 8000
                     },
                     "path": "/predict/event2mind"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-fine-grained-named-entity-recognition",
                        "servicePort": 8000
                     },
                     "path": "/predict/fine-grained-named-entity-recognition"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-named-entity-recognition",
                        "servicePort": 8000
                     },
                     "path": "/predict/named-entity-recognition"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-naqanet-reading-comprehension",
                        "servicePort": 8000
                     },
                     "path": "/predict/naqanet-reading-comprehension"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-nlvr-parser",
                        "servicePort": 8000
                     },
                     "path": "/predict/nlvr-parser"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-open-information-extraction",
                        "servicePort": 8000
                     },
                     "path": "/predict/open-information-extraction"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-quarel-parser-zero",
                        "servicePort": 8000
                     },
                     "path": "/predict/quarel-parser-zero"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-reading-comprehension",
                        "servicePort": 8000
                     },
                     "path": "/predict/reading-comprehension"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-semantic-role-labeling",
                        "servicePort": 8000
                     },
                     "path": "/predict/semantic-role-labeling"
                  },
                  {
                     "backend": {
                        "serviceName": "allennlp-demo-staging-textual-entailment",
                        "servicePort": 8000
                     },
                     "path": "/predict/textual-entailment"
                  },
                  {
                     "backend": {
